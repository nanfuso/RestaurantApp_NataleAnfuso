<h1>Log In!</h1>

<%= form_tag "/login", method: :post do %>

    <div class="form_group">
        <h4>Email: </h4>
        <%= text_field_tag :email %>
    </div>

    <div class="form_group">
        <h4>Password: </h4><%= password_field_tag :password %>
    </div>
<br>
    <%= submit_tag "Log In", :class => "btn btn-default" %>
<% end %>